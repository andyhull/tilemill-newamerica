#hgPoints{
  marker-width:0;
  marker-fill:#0D253F;
  marker-line-color:#333;
  [total>=1][total<=4]{
    marker-width:2;
    marker-fill:#333;
   }
  [total>4][total<=12]{
    marker-width:4;
    marker-fill:#0D253F;
   }
  [total>12][total<=20]{
    marker-width:8;
    marker-fill:#0D253F;
   }
  [total>20]{
    marker-width:15;
    marker-fill:#0D253F;
   }
}