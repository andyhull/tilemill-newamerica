@line: #f4f4f4;

#monthly2011 {
    ::class1[cost_hou_1 <=300]{
        polygon-fill:#fdd7d9;
        line-color: @line;
        line-width: .5;
      }
    ::class2[cost_hou_1 >300][cost_hou_1 <=350]{
        polygon-fill:#f8878c;
        line-color: @line;
        line-width: .5;
      }
    ::class3[cost_hou_1 >350][cost_hou_1 <=400]{
        polygon-fill:#e90e18;
        line-color: @line;
        line-width: .5;
      }
    ::class4[cost_hou_1 >400]{
        polygon-fill:#990910;
        line-color: @line;
        line-width: .5;
      }
}

