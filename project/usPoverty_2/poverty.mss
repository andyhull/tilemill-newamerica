#county09{
  [povRate09 <=10]{
      polygon-fill:#cde0f5;
    }
  [povRate09>10][povRate09 <=15]{
      polygon-fill:#4e92db;
    }
  [povRate09>15][povRate09<=20]{
      polygon-fill:#1e5793;
    }
  [povRate09>20]{
      polygon-fill:#0d253f;
    }
  [zoom >5]{
      ::inner { 
    line-color:#f4f4f4;
    line-opacity:0.4;
    line-join:round;
    line-width:0.5;
  }
   ::outer[zoom>1] { 
    line-color:#999;
    line-width:2;
    line-opacity:0.1;
    line-join:round;
  }
    }
  [zoom < 5]{
      line-width:0;
    }
  [zoom>=8]{
  text-name:'[NAME]';
  text-face-name:'Futura Medium';
  text-size:11;
  text-fill:#a0a0a0; 
  }
}

